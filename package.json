{
  "name": "rpg-web-character-creator",
  "version": "1.1.6",
  "description": "An open source character generator and manager for RPGs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SkyJedi/RPG-Web-Character-Creator.git"
  },
  "keywords": [
    "RPG",
    "FFG",
    "Genesys",
    "character",
    "creator",
    "web"
  ],
  "author": "SkyJedi",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/SkyJedi/RPG-Web-Character-Creator/issues"
  },
  "dependencies": {
    "@firebase/app": "^0.3.3",
    "@firebase/app-types": "^0.3.2",
    "@firebase/firestore": "^0.5.4",
    "ajv": "^6.5.0",
    "bootstrap": "^4.1.1",
    "core-js": "^2.5.7",
    "deepmerge": "^2.1.1",
    "firebase": "^5.0.4",
    "keymaster": "^1.6.2",
    "lodash-es": "^4.17.10",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "popper.js": "^1.14.3",
    "react": "^16.4.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-addons-transition-group": "^15.6.2",
    "react-dom": "^16.4.0",
    "react-dynamic-font": "^1.0.1",
    "react-redux": "^5.0.7",
    "react-scripts": "^1.1.4",
    "react-tabs": "^2.2.2",
    "react-transition-group": "^2.3.1",
    "reactstrap": "^6.1.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "seedrandom": "^2.4.3",
    "sockjs-client": "^1.1.5",
    "sw-rpg-icons": "^1.7.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js"
  },
  "devDependencies": {
    "css-loader": "^0.28.11",
    "style-loader": "^0.21.0"
  }
}
